    "displayName": "displayName",
    "memberOf": "memberOf",
    "mail": "mail",
    "company": "o",
    "thumbnailPhoto": "jpegPhoto",
    "uid": "uid",
    "telephoneNumber": "homephone",
    "sAMAccountName": "uid"
