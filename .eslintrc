{
  "root": true,
  "env": {
    "browser": true,
    "jquery": true,
    "node": true,
    "mocha": true
  },
  "extends": "defaults/configurations/walmart/es5",
  "rules": {
    "no-console": 0,
    "quotes": [2, "single", "avoid-escape"],
    "camelcase": 0,
    "max-statements": 0,
    "func-style": 0,
    "max-len": 0,
    "max-params": [2, 10],
    "operator-assignment": 0,
    "consistent-this": 0
  }
}
